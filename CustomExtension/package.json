{
	"name": "customextension",
	"displayName": "CustomExtension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:customextension.tfePanel",
		"onCommand:customextension.datalogPanel"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "customextension.tfePanel",
				"title": "TFE Panel"
			},
			{
				"command": "customextension.datalogPanel",
				"title": "DataLog Panel"
			},
			{
				"command": "customextension.siteConfigurationPanel",
				"title": "Site Configuration Panel"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "customextension.tfePanel",
					"title": "All panels Side by Side",
					"group": "title"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.5.0",
		"@grpc/proto-loader": "^0.6.9",
		"event-stream": "^4.0.1",
		"fs-backwards-stream": "^1.0.0",
		"google-protobuf": "^3.19.3",
		"requirejs": "^2.3.6",
		"reverse-line-reader": "^0.2.6",
		"util": "^0.12.4"
	}
}
